<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.audio.pandoki"
	name="Pandoki"
	version="0.9.5a"
	provider-name="gominoa, rocky4546, crwolff">
	<requires>
		<import addon="xbmc.python" version="2.25.0"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="default.py">
		<provides>audio</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Pandora Internet Radio</summary>
		<description lang="en">Pandora Internet Radio</description>
		<disclaimer lang="en">Ain't my fault</disclaimer>
		<language></language>
		<platform>all</platform>
		<license>GNU GENERAL PUBLIC LICENSE Version 3, 29 June 2007</license>
		<forum>http://forum.xbmc.org/showthread.php?tid=205547</forum>
		<website></website>
		<email></email>
		<source>https://github.com/crwolff/crwolff-kodi-addons</source>
	</extension>
</addon>

<addon id="repository.crwolff" name="crwolff's Kodi Addons" version="0.1.0" provider-name="crwolff">
	<extension point="xbmc.addon.repository" name="crwolff's Kodi Addon Repository">
		<info compressed="false">http://raw.github.com/crwolff/crwolff-kodi-addons/master/addons.xml</info>
		<checksum>http://raw.github.com/crwolff/crwolff-kodi-addons/master/addons.xml.md5</checksum>
		<datadir zip="true">http://raw.github.com/crwolff/crwolff-kodi-addons/master/repo</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Install crwolff's Addons</summary>
		<description lang="en">Download and install addons by crwolff</description>
		<platform>all</platform>
	</extension>
</addon>
</addons>
